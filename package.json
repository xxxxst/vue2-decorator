		"@rollup/plugin-json": "^4.1.0",
		"@rollup/plugin-node-resolve": "^11.1.0",
		"cross-env": "^7.0.3",
		"rollup": "^2.38.0",
		"rollup-plugin-cleandir": "^1.0.0",
		"rollup-plugin-dts": "3.0.1",
		"rollup-plugin-peer-deps-external": "^2.2.4",
		"rollup-plugin-terser": "^7.0.2",
